{
    "name": "TOSbot",
    "version": "1.0",
    "description": "Analyzes the terms of service of the websites you vist so you don't have to.",
    "manifest_version": 2,
    "browser_action":
    {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"

  }
