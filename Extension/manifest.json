{
    "name": "TOSbot",
    "version": "1.0",
    "description": "Analyzes the terms of service of the websites you vist so you don't have to.",
    "manifest_version": 2,
    "browser_action":
    {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "content_scripts":
    [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ]

  }
